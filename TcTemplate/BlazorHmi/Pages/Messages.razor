@page "/messages"


<RenderableContentControl Context="@Entry.Plc.MAIN._app._mainStation"
                          Presentation="Diagnostics" />

@code {
    protected override void OnInitialized()
    {
        TcoDiagnosticsViewModel.SetDefaultCategory(eMessageCategory.Trace);
        TcoDiagnosticsView.SetDefaultDepth(7);
        TcoDiagnosticsView.SetDiagnosticsUpdateInterval(200);
        base.OnInitialized();
    }
}
